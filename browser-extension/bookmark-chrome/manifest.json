{
    "manifest_version": 3,
    "name": "Bookmark Extension",
    "version": "1.0",
    "description": "Turns the background red.",
    "permissions": ["activeTab", "tabs", "scripting", "storage"],
    "action": {
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    }
}